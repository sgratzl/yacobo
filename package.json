{
  "name": "yacobo",
  "version": "0.2.0",
  "description": "Test",
  "private": true,
  "author": {
    "name": "Samuel Gratzl",
    "email": "sam@sgratzl.com",
    "url": "https://www.sgratzl.com"
  },
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "lint": "eslint src --ext ts,tsx && prettier src/** --check",
    "fix": "eslint src --ext ts,tsx --fix && prettier src/** --write",
    "build": "cross-env NEXT_TELEMETRY_DISABLED=1 next build",
    "canvas:fix": "yum install gcc-c++ cairo-devel pango-devel libjpeg-turbo-devel giflib-devel libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid}.so.1 node_modules/canvas/build/Release/",
    "vercel-build": "yarn canvas:fix && yarn build",
    "start": "next start",
    "cli": "ts-node --transpile-only -r tsconfig-paths/register --project tsconfig.cli.json ./src/cli/index.ts"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@popperjs/core": "^2.5.3",
    "@types/cors": "^2.8.8",
    "@types/d3-dsv": "^2.0.0",
    "@types/fluent-ffmpeg": "^2.1.16",
    "@types/react-vega": "^6.0.0",
    "@types/redis": "^2.8.28",
    "@types/swagger-ui-react": "^3.23.3",
    "@types/use-persisted-state": "^0.3.0",
    "antd": "^4.7.2",
    "canvas": "^2.6.1",
    "cors": "^2.8.5",
    "cross-fetch": "^3.0.6",
    "d3-dsv": "^2.0.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "next": "^9.5.5",
    "rc-resize-observer": "^0.2.6",
    "react": "17.0.0",
    "react-dom": "17.0.0",
    "react-vega": "^7.4.1",
    "react-virtual": "^2.2.8",
    "redis": "^3.0.2",
    "swagger-ui-react": "^3.35.2",
    "swr": "^0.3.6",
    "us-atlas": "^3.0.0",
    "use-persisted-state": "^0.3.0",
    "use-ssr": "^1.0.23",
    "vega": "^5.17.3",
    "vega-lite": "^4.17.0",
    "vega-scale": "^7.1.1",
    "yacobo-font-canvas-helper": "file:./yacobo-font-canvas-helper"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt-cli": "^11.0.0",
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "@types/node": "^14.14.0",
    "@types/react": "^16.9.53",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "resolutions_d": {
    "webpack": "^5.0.0-rc.0"
  }
}
